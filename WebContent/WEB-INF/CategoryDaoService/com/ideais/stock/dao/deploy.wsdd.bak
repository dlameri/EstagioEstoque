      <typeMapping
        xmlns:ns="http://dao.stock.ideais.com"
        qname="ns:>findAllResponse"
        type="java:com.ideais.stock.dao.FindAllResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.stock.ideais.com"
        qname="ns:>update"
        type="java:com.ideais.stock.dao.Update"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://domain.stock.ideais.com"
        qname="ns:Category"
        type="java:com.ideais.stock.domain.Category"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://dao.stock.ideais.com"
        qname="ns:>updateResponse"
        type="java:com.ideais.stock.dao.UpdateResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>

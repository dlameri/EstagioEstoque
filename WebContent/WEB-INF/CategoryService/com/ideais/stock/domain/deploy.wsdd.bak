<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CategoryService WSDL service -->

  <service name="Category" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://domain.stock.ideais.com"/>
      <parameter name="wsdlServiceElement" value="CategoryService"/>
      <parameter name="schemaQualified" value="http://domain.stock.ideais.com"/>
      <parameter name="wsdlServicePort" value="Category"/>
      <parameter name="className" value="com.ideais.stock.domain.CategorySoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Category"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getName" qname="operNS:getName" xmlns:operNS="http://domain.stock.ideais.com" returnQName="retNS:getNameReturn" xmlns:retNS="http://domain.stock.ideais.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="setName" qname="operNS:setName" xmlns:operNS="http://domain.stock.ideais.com" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://domain.stock.ideais.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getId" qname="operNS:getId" xmlns:operNS="http://domain.stock.ideais.com" returnQName="retNS:getIdReturn" xmlns:retNS="http://domain.stock.ideais.com" returnType="rtns:long" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="setId" qname="operNS:setId" xmlns:operNS="http://domain.stock.ideais.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://domain.stock.ideais.com" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getName setName setId getId"/>

  </service>
</deployment>
